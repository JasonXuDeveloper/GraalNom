public class ObjectTest
{
    public static fun Main() : Int
    {
        MyObj obj = new MyObj(0, "Hello");
        MyObj obj2 = new MyObj(1, "World");
        MyObj obj3 = new MyObj(3, "!");
        obj.Print();
        obj2.Print();
        obj3.y.Print();

        return obj.x + MyObj.GetX(obj2) + obj3.x;
    }
}
public class MyObj
{
    public Int x;
    public String y;

    public constructor(Int i, String s)
    {
        x = i;
        y = s;
    }

    public fun Print() : Void
    {
        x = x + 1;
        y.Print();
    }

    public static fun GetX(MyObj obj) : Int
    {
        return obj.x;
    }
}