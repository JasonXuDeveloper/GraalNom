public class ObjectTest
{
    public static fun Main() : Int
    {
        MyObj obj = new MyObj(1, "Hello");
        MyObj obj2 = new MyObj(2, "World");
        MyObj obj3 = new MyObj(3, "!");
        obj.y.Print();
        obj2.y.Print();
        obj3.y.Print();

        return obj.x + obj2.x + obj3.x;
    }
}
public class MyObj
{
    public Int x;
    public String y;

    public constructor(Int i, String s)
    {
        x = i;
        y = s;
    }
}